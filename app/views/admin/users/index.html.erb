<h1>Rotten Mangoes</h1>
<%= link_to "Create a user.", new_admin_user_path %>
<hr>
<%= paginate @users %>
<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Edit</th>
    <th>Delete</th>
    <th>Impersonate</th>
  </tr>
  <% @users.each do |user| %>
  <tr>
    <td><%= user.full_name %></td>
    <td><%= user.email %></td>
    <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
    <td><%= link_to "Delete", admin_user_path(user), method: "delete" %></td>
    <td><%= link_to "Impersonate", session_path, method: "puts" %></td>
  </tr>
  <% end %>
</table>
